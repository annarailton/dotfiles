[user]
	email = annarailton@github.com
	name = annarailton
	signingkey = 6559695657CC220B
[core]
	editor = nano -r 72
[push]
	default = simple
[alias]
    a = add
    st = status -s
    stt = status
    cm = commit -m
    cn = commit -n -m
    c = commit
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    up = !git fetch origin && git rebase origin/master --autostash
    logn = log --name-only
    b = for-each-ref --sort=committerdate refs/heads/ --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) - %(color:red)%(objectname:short)%(color:reset) - %(contents:subject) - %(authorname) (%(color:green)%(committerdate:relative)%(color:reset))'

[filesystem "Private Build|1.8.0_222|/dev/mapper/ubuntu--vg-root"]
	timestampResolution = 1001 microseconds
	minRacyThreshold = 4577 microseconds
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[filesystem "Private Build|1.8.0_232|/dev/mapper/ubuntu--vg-root"]
	timestampResolution = 1001 microseconds
	minRacyThreshold = 7294 microseconds
